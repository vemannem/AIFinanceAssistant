# AI Finance Assistant Frontend - Environment Configuration
# Copy this file to .env.local and update with your backend server URL

# Backend API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Optional: API Keys (if needed)
VITE_API_KEY=

# Optional: App Environment
VITE_ENV=development

# Optional: Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_WEBSOCKET_STREAMING=true
VITE_ENABLE_PORTFOLIO_ANALYSIS=true

# Optional: Logging
VITE_LOG_LEVEL=info
VITE_ENABLE_DEBUG=false
